{
  "id": "vfox",
  "version": "0.2.4",
  "name": "vfox",
  "documentationURL": "https://github.com/ccavolt/devcontainer-features/tree/main/src/vfox",
  "description": "vfox is a cross-platform, extensible version manager.",
  "options": {
    "version": {
      "default": "latest",
      "description": "Select the version of vfox to install.",
      "proposals": [
        "latest",
        "0.4.2"
      ],
      "type": "string"
    },
    "user": {
      "default": "root",
      "description": "Name of user devcontainer will run as.",
      "proposals": [
        "root",
        "vscode"
      ],
      "type": "string"
    },
    "shell": {
      "default": "bash",
      "description": "Shell used.",
      "proposals": [
        "bash",
        "fish",
        "zsh"
      ],
      "type": "string"
    }
  }
}
